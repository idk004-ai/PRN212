<Window x:Class="QuanLiKhiThai.AssignInspectorWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuanLiKhiThai"
        mc:Ignorable="d"
        Title="AssignInspectorWindow" Height="450" Width="800">
    <Grid Background="Beige">
        <StackPanel Margin="20">
            <TextBlock Text="Assign Inspector" FontSize="20" FontWeight="Bold" Margin="0,0,0,20"/>
            <ComboBox Name="comboBoxInspectors" Width="200" Margin="0,0,0,20"/>
            <Button Content="Assign" Width="100" HorizontalAlignment="Center" Click="AssignButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
